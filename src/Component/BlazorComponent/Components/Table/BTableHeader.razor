@namespace BlazorComponent
@inherits BDomComponentBase

<thead class="@CssProvider.GetClass(typeof(BTableHeader))" id="@Id" @ref="Ref">
    <tr>
        @if (Headers != null)
        {
            @foreach (var header in Headers)
            {
                <th>@header</th>
            }
        }
    </tr>
</thead>
