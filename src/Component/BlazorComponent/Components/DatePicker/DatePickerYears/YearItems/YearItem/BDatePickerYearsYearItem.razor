@namespace BlazorComponent
@typeparam TDatePickerYears
@inherits ComponentAbstractBase<TDatePickerYears>

    @{
    var active = Value == Year;
    <li @key="Year" class="@CssProvider.GetClass("year-item",data:active)" @onclick="HandleOnYearItemClick">
        @Formatter(new DateOnly(Year,1,1))
    </li>
}

